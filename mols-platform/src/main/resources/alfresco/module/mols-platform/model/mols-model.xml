<?xml version="1.0" encoding="UTF-8"?>
<model name="ml:molsmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<!-- Optional meta-data about the model -->
	<description>
		Document Model For Mols
	</description>
	<author>
		Brian
	</author>
	<version>
		1.0
	</version>
	<imports>
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<!-- Import Alfresco System Model Definitions -->
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
	</imports>
	<!-- Custom namespace for the mols -->
	<namespaces>
		<namespace uri="http://www.mols.com/model/mols/1.0" prefix="ml" />
	</namespaces>
	<constraints>
	</constraints>
	<!--
	===============================================================================================================
	Constraints, Types, and Aspects go here...
	-->
	<types>
		<!--
		ACME Enterprise-wide Document root type.
		All other custom document types would extend this one.
		-->
		<type name="ml:infrast_reports_holder">
			<title>
				Infrastructure Projects Report Folder
			</title>
			<description>
				Contains Report Volumes Folders
			</description>
			<parent>cm:folder</parent>
			<properties>
				<property name="ml:project_name">
					<title>
						Project Name
					</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="ml:volume">
					<title>
						Project Volume/Phase
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:signatory">
					<title>
						Signatory
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
                            <constraint type="LIST">
                                <parameter name="allowedValues">
                                    <list>
                                        <value>A J BWIRAGURA</value>
                                        <value>NSAMBA GAYIINA</value>
                                        <value>LUCY KABEGE</value>
                                        <value>MAGALA</value>
                                        <value>GILBERT KERMUNDU</value>
                                        <value>MUKWAYA GEOFREY</value>
                                        <value>KATO TONY</value>
                                        <value>NYUMBA ANDREW</value>
                                        <value>NATUKUNDA DOROTHY</value>
                                        <value>GOGEZI SWAD YASIN</value>
                                        <value>OTHERS</value>
                                    </list>
                                </parameter>
                            </constraint>
                    </constraints>
				</property>
			</properties>
			<mandatory-aspects>
				<aspect>ml:report_details</aspect>
				<aspect>ml:report_holder_details</aspect>
			</mandatory-aspects>
		</type>
		<type name="ml:reports_holder">
			<title>
				General Valuation Reports Folder
			</title>
			<description>
				Contains Report Volumes Folders
			</description>
			<parent>cm:folder</parent>
			<mandatory-aspects>
				<aspect>ml:report_holder_details</aspect>
			</mandatory-aspects>
		</type>
		<type name="ml:report_volume">
			<title>
				Reports Volume
			</title>
			<description>
				Contains various valuation reports
			</description>
			<parent>cm:folder</parent>
			<mandatory-aspects>
				<aspect>ml:reports_vol</aspect>
				<aspect>ml:report_holder_details</aspect>
			</mandatory-aspects>
		</type>
		<type name="ml:valuation_report">
			<title>
				Report
			</title>
			<parent>cm:content</parent>
			<mandatory-aspects>
				<aspect>ml:report_details</aspect>
				<aspect>ml:report_holder_details</aspect>
			</mandatory-aspects>
		</type>
		<type name="ml:project_report">
			<title>
				Report
			</title>
			<parent>cm:content</parent>
			<properties>
			</properties>
			<mandatory-aspects>
			</mandatory-aspects>
		</type>
		<type name="ml:stamp_duty">
			<title>Stamp Duty</title>
			<parent>cm:content</parent>
			<properties>
				<property name="ml:plot">
					<title>Plot</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:date">
					<title>Date</title>
					<type>d:date</type>
					<mandatory>true</mandatory>
				</property>
				<property name="ml:folio">
					<title>Folio</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:user">
					<title>User</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<constraints>
                            <constraint type="LIST">
                                <parameter name="allowedValues">
                                    <list>
                                        <value>Residential</value>
                                        <value>Commercial</value>
                                        <value>Agricultural</value>
                                        <value>Industrial</value>
                                        <value>Institutional</value>
                                        <value>Infrastructure</value>
                                        <value>Mixed Use</value>
                                    </list>
                                </parameter>
                            </constraint>
                    </constraints>
				</property>
				<property name="ml:land_area">
					<title>Land Area</title>
					<type>d:int</type>
					<mandatory>true</mandatory>
				</property>
				<property name="ml:land_development">
					<title>Land Development</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<constraints>
						<constraint type="LIST">
							<parameter name="allowedValues">
								<list>
									<value>Developed</value>
									<value>Vacant</value>
								</list>
							</parameter>
						</constraint>
					</constraints>
				</property>
				<property name="ml:land_value">
					<title>Land Value</title>
					<type>d:int</type>
					<mandatory>true</mandatory>
				</property>

			</properties>
		</type>
	</types>
	<aspects>
		<!--
		A document can have security classification applied and
		faceted search is specifically enabled for best performance and we change
		default index config to not tokenize the value.
		-->
		<aspect name="ml:report_details">
			<title>
				Contains Report Details
			</title>
			<description>
				This represents a valuation report
			</description>
			<properties>
				<property name="ml:report_approval_date">
					<title>
						Valuation Report Submission Date
					</title>
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:report_subject">
					<title>
						Valuation Report Subject
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:report_agency">
					<title>
						Agency
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:valuation_type">
					<title>
						Type / Purpose of Valuation
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
                            <constraint type="LIST">
                                <parameter name="allowedValues">
                                    <list>
                                        <value>COMPENSATION VALUATION</value>
                                        <value>STAMP DUTY VALUATION</value>
                                        <value>MARKET VALUATION</value>
                                        <value>PROBATE VALUATION</value>
                                        <value>RENTAL VALUATION</value>
                                        <value>DETERMINATION OF TERMS (PREMIUM AND GROUND RENT FOR LEASES)</value>
                                        <value>GROUND RENT REVISION</value>
                                        <value>VALUATION FOR PURPOSE</value>
                                        <value>VALUATION FOR SALE OR DISPOSAL</value>
                                        <value>VALUATION FOR LEASEHOLD CONVERSION INTO FREEHOLD</value>
                                        <value>VALUATION FOR CAPITAL GAINS TAX</value>
                                        <value>VALUATION FOR AUDIT AND FINANCIAL REPORTING</value>
                                        <value>LOCAL GOVERNMENT RATING VALUATIONS</value>
                                        <value>INSURANCE VALUATION</value>
                                    </list>
                                </parameter>
                            </constraint>
                    </constraints>
				</property>
				<property name="ml:location">
					<title>
						Location
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="ml:report_holder_details">
			<title>
				ype / Purpose of Valuation
				Contains Report Holder Details
			</title>
			<description>
				This details a folder that Contains report volumes
			</description>
			<properties>
				<property name="ml:holder_reference_no">
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="ml:reports_vol">
			<title>
				Reports File
			</title>
			<description>
				This details a folder that Contains report
			</description>
			<properties>
				<!--
				<property name="ml:vol_number">
				<type>
				d:int
				</type>
				<mandatory>
				true
				</mandatory>
				</property>
				-->
				<property name="ml:open_date">
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:close_date">
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="ml:instruction_letter_details">
			<title>
				Instruction Letter Aspect
			</title>
			<description>
				Instruction Letter Details
			</description>
			<properties>
				<!--
				<property name="ml:letter_author">
				<title>Instruction letter Author</title>
				<type>d:text</type>
				<mandatory>true</mandatory>
				</property>
				-->
				<property name="ml:letter_quote_no">
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ml:letter_from_address">
					<title>
						Sender's Address
					</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<!--
				<property name="ml:date_sent">
				<title>Sender's Date</title>
				<type>d:date</type>
				<mandatory>true</mandatory>
				</property>
				-->
				<!--
				<property name="ml:date_recieved">
				<title>Date of Reciept by Mols</title>
				<type>d:date</type>
				<mandatory>true</mandatory>
				</property>
				-->
				<!--
				<property name="ml:letter_subject">
				<title>Valuation Subject</title>
				<type>d:text</type>
				<mandatory>true</mandatory>
				</property>
				-->
				<!--
				<property name="ml:project_title">
				<title>Valuation Project</title>
				<type>d:text</type>
				<mandatory>false</mandatory>
				</property>
				-->
			</properties>
		</aspect>
	</aspects>
</model>
